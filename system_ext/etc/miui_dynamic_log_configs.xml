<?xml version="1.0" encoding="UTF-8"?>
<dynamic_log_config>
    <moudle name="package"
        cmd="pm"
        debugflags="DEBUG_SETTINGS,DEBUG_PREFERRED,DEBUG_UPGRADE,DEBUG_DOMAIN_VERIFICATION,DEBUG_BACKUP,DEBUG_INSTALL,DEBUG_REMOVE,DEBUG_PACKAGE_INFO,DEBUG_INTENT_MATCHING,DEBUG_PACKAGE_SCANNING,DEBUG_VERIFY,DEBUG_PERMISSIONS" />
    <moudle name="activity"
        cmd="am"
        debugflags="DEBUG_BACKGROUND_CHECK,DEBUG_BROADCAST,DEBUG_BROADCAST_BACKGROUND,DEBUG_BROADCAST_LIGHT,DEBUG_BROADCAST_DEFERRAL,DEBUG_PROVIDER,DEBUG_SERVICE,DEBUG_FOREGROUND_SERVICE,DEBUG_SERVICE_EXECUTING,DEBUG_ALLOWLISTS,DEBUG_RECENTS,DEBUG_RECENTS_TRIM_TASKS,DEBUG_ROOT_TASK,DEBUG_SWITCH,DEBUG_TRANSITION DEBUG_VISIBILITY,DEBUG_APP,DEBUG_IDLE,DEBUG_RELEASE,DEBUG_USER_LEAVING,DEBUG_PERMISSIONS_REVIEW,DEBUG_RESULTS,DEBUG_ACTIVITY_STARTS,DEBUG_CLEANUP,DEBUG_METRICS" />
    <moudle name="window"
        cmd="wm"
        debugflags="TEST_GROUP,WM_DEBUG_ADD_REMOVE,WM_DEBUG_ANIM,WM_DEBUG_APP_TRANSITIONS,WM_DEBUG_APP_TRANSITIONS_ANIM,WM_DEBUG_BOOT,WM_DEBUG_CONFIGURATION,WM_DEBUG_CONTAINERS,WM_DEBUG_DRAW,WM_DEBUG_FOCUS,WM_DEBUG_FOCUS_LIGHT,WM_DEBUG_IME,WM_DEBUG_IMMERSIVE,WM_DEBUG_KEEP_SCREEN_ON,WM_DEBUG_LOCKTASK,WM_DEBUG_ORIENTATION,WM_DEBUG_RECENTS_ANIMATIONS,WM_DEBUG_REMOTE_ANIMATIONS,WM_DEBUG_RESIZE,WM_DEBUG_SCREEN_ON,WM_DEBUG_STARTING_WINDOW,WM_DEBUG_STATES,WM_DEBUG_SWITCH,WM_DEBUG_SYNC_ENGINE,WM_DEBUG_TASKS,WM_DEBUG_WALLPAPER,WM_DEBUG_WINDOW_INSETS,WM_DEBUG_WINDOW_MOVEMENT,WM_DEBUG_WINDOW_ORGANIZER,WM_SHOW_SURFACE_ALLOC,WM_SHOW_TRANSACTIONS,WM_SHELL_SPLIT_SCREEN,WM_SHELL_TASK_ORG,WM_SHELL_RECENT_TASKS" />
    <moudle name="input_method"
        cmd = "dumpsys input_method"
        debugflags="IMS,IMM,IMMS" />
    <moudle name="appwidget"
        cmd = "dumpsys appwidget"
        debugflags="DEBUG" />
    <moudle name="winscope_wm"
        debugflags="debug"
        enbaleCmd="cmd window tracing start"
        disbaleCmd="cmd window tracing stop" />
    <moudle name="winscope_sf"
        debugflags="debug"
        enbaleCmd="service call SurfaceFlinger 1025 i32 1"
        disbaleCmd="service call SurfaceFlinger 1025 i32 0" />
    <moudle name="stability_cancelcuthprof"
        debugflags="debug"
        enbaleCmd="setprop persist.sys.disable_minihprof true"
        disbaleCmd="setprop persist.sys.disable_minihprof false" />
    <moudle name="stability_grabhprof"
        debugflags="debug"
        enbaleCmd="setprop persist.sys.debug.enable_scout_memory_monitor true"
        disbaleCmd="setprop persist.sys.debug.enable_scout_memory_monitor false" />
    <moudle name="power"
        debugflags="debug"
        enbaleCmd="setprop debug.miui.power.dbg 255"
        disbaleCmd="setprop debug.miui.power.dbg 0" />
    <moudle name="power_autobrightness"
        debugflags="debug"
        enbaleCmd="cmd display ab-logging-enable"
        disbaleCmd="cmd display ab-logging-disable" />
    <moudle name="power_display"
        debugflags="debug"
        enbaleCmd="setprop debug.miui.display.dbg 255"
        disbaleCmd="setprop debug.miui.display.dbg 0" />
    <moudle name="input"
        cmd = "dumpsys input"
        debugflags="MIUIInput,InputDispatcher,InputReader,InputTransport"/>
    <moudle name="notification"
        debugflags="debug"
        enbaleCmd="cmd notification enhance_log true"
        disbaleCmd="cmd notification enhance_log false" />
</dynamic_log_config>
