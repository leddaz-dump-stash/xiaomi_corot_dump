			<!--HAL Dump-->
			<Command id="hal_dl_dump" name="HAL DL" ui_type="checkbox" checked="1" unchecked="0" cmd_type="set_property" property="vendor.streamout.pcm.dump" default="0"/>
			<Command id="hal_ul_dump" name="HAL UL" ui_type="checkbox" checked="1" unchecked="0" cmd_type="set_property" property="vendor.streamin.pcm.dump" default="0"/>
			
			<!--ADSP Dump-->
			<Command id="adsp_dl_dump" name="ADSP DL" ui_type="checkbox" checked="1" unchecked="0" cmd_type="set_property" property="vendor.streamout.dsp.dump" default="0"/>
			<Command id="adsp_ul_dump" name="ADSP UL" ui_type="checkbox" checked="1" unchecked="0" cmd_type="set_property" property="vendor.streamin.dsp.dump" default="0"/>
			
			<!--MD Dump-->
			<Command id="md_vm_config" name="MD VM Config" ui_type="checkbox" checked="1" unchecked="0" cmd_type="set_property" property="persist.vendor.audiohal.vm_cfg" default="0"/>
			<Command id="md_speech_para_on" name="MD Speech Para" ui_type="hidden" button_action="adb shell SET=`AudioSetParam APP_GET_PARAM#SpeechGeneral#CategoryLayer,EPL_On#speech_common_para 2>&#38;1 | cut -d '=' -f 2`; AudioSetParam APP_SET_PARAM=SpeechGeneral#CategoryLayer,Common#speech_common_para#${SET}" cmd_type="adb shell"/>
			<Command id="md_speech_para_off" name="MD Speech Para off" ui_type="hidden" button_action="adb shell SET=`AudioSetParam APP_GET_PARAM#SpeechGeneral#CategoryLayer,EPL_Off#speech_common_para 2>&#38;1 | cut -d '=' -f 2`; AudioSetParam APP_SET_PARAM=SpeechGeneral#CategoryLayer,Common#speech_common_para#${SET}" cmd_type="adb shell"/>
			<Command id="md_debug_on" name="MD Debug Info" ui_type="hidden" button_action="adb shell SET=`AudioSetParam APP_GET_PARAM#SpeechGeneral#CategoryLayer,EPL_On#debug_info 2>&#38;1 | cut -d '=' -f 2`; AudioSetParam APP_SET_PARAM=SpeechGeneral#CategoryLayer,Common#debug_info#${SET}" cmd_type="adb shell"/>
			<Command id="md_debug_off" name="MD Debug Info off" ui_type="hidden" button_action="adb shell SET=`AudioSetParam APP_GET_PARAM#SpeechGeneral#CategoryLayer,EPL_Off#debug_info 2>&#38;1 | cut -d '=' -f 2`; AudioSetParam APP_SET_PARAM=SpeechGeneral#CategoryLayer,Common#debug_info#${SET}" cmd_type="adb shell"/>
			<Command id="save_xml" name="" ui_type="hidden" button_action="adb shell AudioSetParam APP_SAVE_XML=SpeechGeneral" cmd_type="adb shell"/>
		</Group>
	</Category>
