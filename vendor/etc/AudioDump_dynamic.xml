			<Command id="af_resampler_dump" name="AF Resampler" ui_type="checkbox" checked="1" unchecked="0" cmd_type="set_property" property="vendor.af.resampler.pcm" default="0"/>
			<Command id="af_resampler_dump_set_param_on" name="" ui_type="hidden" button_action="adb shell AudioSetParam vendor.af.resampler.pcm=1" cmd_type="adb shell"/>
			<Command id="af_resampler_dump_set_param_off" name="" ui_type="hidden" button_action="adb shell AudioSetParam vendor.af.resampler.pcm=0" cmd_type="adb shell"/>

			<Command id="af_aaudio_dump" name="AF AAudio" ui_type="checkbox" checked="1" unchecked="0" cmd_type="set_property" property="vendor.aaudio.pcm" default="0"/>
			<Command id="af_aaudio_dump_set_param_on" name="" ui_type="hidden" button_action="adb shell AudioSetParam vendor.aaudio.pcm=1" cmd_type="adb shell"/>
			<Command id="af_aaudio_dump_set_param_off" name="" ui_type="hidden" button_action="adb shell AudioSetParam vendor.aaudio.pcm=0" cmd_type="adb shell"/>

			<Command id="af_effect_dump" name="AF Effect" ui_type="checkbox" checked="1" unchecked="0" cmd_type="set_property" property="vendor.af.effect.pcm" default="0"/>
			<Command id="af_effect_dump_set_param_on" name="" ui_type="hidden" button_action="adb shell AudioSetParam vendor.af.effect.pcm=1" cmd_type="adb shell"/>
			<Command id="af_effect_dump_set_param_off" name="" ui_type="hidden" button_action="adb shell AudioSetParam vendor.af.effect.pcm=0" cmd_type="adb shell"/>

			<Command id="af_dump_log" name="AF Dump" ui_type="checkbox" checked="1" unchecked="0" cmd_type="set_property" property="vendor.af.dumplog" default="0"/>
			<Command id="af_dump_log_set_param_on" name="" ui_type="hidden" button_action="adb shell AudioSetParam vendor.af.dumplog=1" cmd_type="adb shell"/>
			<Command id="af_dump_log_set_param_off" name="" ui_type="hidden" button_action="adb shell AudioSetParam vendor.af.dumplog=0" cmd_type="adb shell"/>

			<!--AF UL Dump-->
			<Command id="af_record_dump" name="AF Record" ui_type="checkbox" checked="1" unchecked="0" cmd_type="set_property" property="vendor.af.record.dump.pcm" default="0"/>
			<Command id="af_record_dump_set_param_on" name="" ui_type="hidden" button_action="adb shell AudioSetParam vendor.af.record.dump.pcm=1" cmd_type="adb shell"/>
