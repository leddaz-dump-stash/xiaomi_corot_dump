	<Category name="Current Dump Setting">
		<Group name="" ui_type="default">
			<!--AT Dump-->
			<Command id="at_dump" name="AT" ui_type="checkbox" checked="1" unchecked="0" cmd_type="set_property" property="vendor.af.track.pcm" default="0"/>
			<Command id="at_dump_set_param_on" name="" ui_type="hidden" button_action="adb shell AudioSetParam vendor.af.track.pcm=1" cmd_type="adb shell"/>
			<Command id="at_dump_set_param_off" name="" ui_type="hidden" button_action="adb shell AudioSetParam vendor.af.track.pcm=0" cmd_type="adb shell"/>

			<!--AF DL Dump-->
			<Command id="af_mixer_dump" name="AF Mixer" ui_type="checkbox" checked="1" unchecked="0" cmd_type="set_property" property="vendor.af.mixer.end.pcm" default="0"/>
			<Command id="af_mixer_dump_set_param_on" name="" ui_type="hidden" button_action="adb shell AudioSetParam vendor.af.mixer.end.pcm=1" cmd_type="adb shell"/>
